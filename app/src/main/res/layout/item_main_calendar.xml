<?xml version="1.0" encoding="utf-8"?>
<!--메인화면 달력프레임 내 들어갈 내용-->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="23dp"
    android:layout_marginHorizontal="39dp">

    <ImageView
        android:id="@+id/item_main_cal_left_iv"
        android:layout_width="12.8dp"
        android:layout_height="15.4dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="30dp"
        android:src="@drawable/item_arrow_left"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_sun_tv"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_month_tv"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:id="@+id/item_main_cal_year_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:text="----년"
        android:textColor="#a7a7a7"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_month_tv"
        app:layout_constraintLeft_toLeftOf="@id/item_main_cal_month_tv"
        app:layout_constraintRight_toRightOf="@id/item_main_cal_month_tv"
        app:layout_constraintTop_toTopOf="parent"></TextView>

    <TextView
        android:id="@+id/item_main_cal_month_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="--월"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_left_iv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_left_iv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_right_iv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_left_iv"></TextView>

    <ImageView
        android:id="@+id/item_main_cal_right_iv"
        android:layout_width="12.8dp"
        android:layout_height="15.4dp"
        android:layout_marginLeft="32dp"
        android:layout_marginBottom="30dp"
        android:src="@drawable/item_arrow_right"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_month_tv"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>
    <!--    좌우버튼과 년월 표시-->


    <TextView
        android:id="@+id/item_main_cal_sun_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="일"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w1_rv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_mon_tv"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_left_iv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_mon_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="월"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_sun_tv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_tue_tv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_tue_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="화"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_mon_tv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_wed_tv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_wed_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="수"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_tue_tv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_thu_tv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_thu_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="목"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_wed_tv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_fri_tv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_fri_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="금"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_thu_tv"
        app:layout_constraintRight_toLeftOf="@id/item_main_cal_sat_tv"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>

    <TextView
        android:id="@+id/item_main_cal_sat_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bmeuljirottf"
        android:text="토"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/item_main_cal_sun_tv"
        app:layout_constraintLeft_toRightOf="@id/item_main_cal_fri_tv"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/item_main_cal_sun_tv"></TextView>
    <!--요일표시-->

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w1_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w2_rv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_sun_tv"></androidx.recyclerview.widget.RecyclerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w2_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w3_rv"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_w1_rv"></androidx.recyclerview.widget.RecyclerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w3_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w4_rv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_w2_rv"></androidx.recyclerview.widget.RecyclerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w4_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w5_rv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_w3_rv"></androidx.recyclerview.widget.RecyclerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w5_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/item_main_cal_w6_rv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_w4_rv"></androidx.recyclerview.widget.RecyclerView>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_main_cal_w6_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_main_cal_w5_rv"
        app:layout_constraintVertical_chainStyle="packed">
    </androidx.recyclerview.widget.RecyclerView>

    <!--    각 주 표시(총 6주)-->

</androidx.constraintlayout.widget.ConstraintLayout>