<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/board_myboard_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My 게시판"
        android:textSize="30sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        />

    <ImageView
        android:id="@+id/board_myboard_mypost_ic"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_launcher_background"
        android:layout_margin="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/board_myboard_title_tv"
        />

    <TextView
        android:id="@+id/board_myboard_mypost_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="내가 쓴 게시글"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/board_myboard_mypost_ic"
        app:layout_constraintTop_toTopOf="@id/board_myboard_mypost_ic" />

    <ImageView
        android:id="@+id/board_myboard_mycomment_ic"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_launcher_background"
        android:layout_margin="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/board_myboard_mypost_ic"
        />

    <TextView
        android:id="@+id/board_myboard_mycomment_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="내가 쓴 댓글"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/board_myboard_mycomment_ic"
        app:layout_constraintTop_toTopOf="@+id/board_myboard_mycomment_ic" />

    <ImageView
        android:id="@+id/board_myboard_postrecommending_ic"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_launcher_background"
        android:layout_margin="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/board_myboard_mycomment_ic"
        />

    <TextView
        android:id="@+id/board_myboard_postrecommending_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="내가 추천한 게시글"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/board_myboard_postrecommending_ic"
        app:layout_constraintTop_toTopOf="@+id/board_myboard_postrecommending_ic" />

    <ImageView
        android:id="@+id/board_myboard_commentrecommending_ic"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_launcher_background"
        android:layout_margin="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/board_myboard_postrecommending_ic"
        />

    <TextView
        android:id="@+id/board_myboard_commentrecommending_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="내가 추천한 댓글"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/board_myboard_commentrecommending_ic"
        app:layout_constraintTop_toTopOf="@+id/board_myboard_commentrecommending_ic"
        />

    <ImageView
        android:id="@+id/board_myboard_userblocked_ic"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_launcher_background"
        android:layout_margin="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/board_myboard_commentrecommending_ic"
        />

    <TextView
        android:id="@+id/board_myboard_userblocked_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="차단한 사용자"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/board_myboard_userblocked_ic"
        app:layout_constraintTop_toTopOf="@+id/board_myboard_userblocked_ic" />


</androidx.constraintlayout.widget.ConstraintLayout>