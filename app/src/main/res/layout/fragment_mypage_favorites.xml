<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="true"
    android:orientation="vertical"
    android:overScrollMode="never"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/mypagefav_user_iv"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/user"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"
            />
        <TextView
            android:id="@+id/mypagefav_user_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="user_name"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="@id/mypagefav_user_iv"
            app:layout_constraintBottom_toBottomOf="@id/mypagefav_user_iv"
            app:layout_constraintStart_toEndOf="@id/mypagefav_user_iv"
            android:layout_marginStart="10dp"
            />

        <TextView
            android:id="@+id/mypagefav_part_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="부위별 즐겨찾기"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_user_iv"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            />

        <Button
            android:id="@+id/mypagefav_sholder_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="어깨"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_back_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="20dp"
            android:layout_marginStart="40dp"
            />

        <Button
            android:id="@+id/mypagefav_back_bt"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="등"
            app:layout_constraintStart_toEndOf="@id/mypagefav_sholder_bt"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_abs_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="20dp"
            />

        <Button
            android:id="@+id/mypagefav_abs_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="복근"
            app:layout_constraintStart_toEndOf="@id/mypagefav_back_bt"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_biceps_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="20dp"
            />

        <Button
            android:id="@+id/mypagefav_biceps_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="이두"
            app:layout_constraintStart_toEndOf="@id/mypagefav_abs_bt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="40dp"
            />

        <Button
            android:id="@+id/mypagefav_leg_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="하체"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_full_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="70dp"
            android:layout_marginStart="40dp"
            />

        <Button
            android:id="@+id/mypagefav_full_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="전신"
            app:layout_constraintStart_toEndOf="@id/mypagefav_leg_bt"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_aerobic_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="70dp"
            />

        <Button
            android:id="@+id/mypagefav_aerobic_bt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="유산소"
            app:layout_constraintStart_toEndOf="@id/mypagefav_full_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="40dp"
            />

        <Button
            android:id="@+id/mypagefav_chest_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="가슴"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_triceps_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            android:layout_marginTop="120dp"
            android:layout_marginStart="40dp"
            />
        <Button
            android:id="@+id/mypagefav_triceps_bt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="삼두"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            app:layout_constraintEnd_toStartOf="@id/mypagefav_hip_bt"
            app:layout_constraintStart_toEndOf="@id/mypagefav_chest_bt"
            android:layout_marginTop="120dp"
            android:layout_marginEnd="40dp"
            />

        <Button
            android:id="@+id/mypagefav_hip_bt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="엉덩이"
            app:layout_constraintStart_toEndOf="@id/mypagefav_triceps_bt"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_part_tv"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="120dp"
            android:layout_marginEnd="40dp"
            />
        <LinearLayout
            android:id="@+id/mypagefav_llo"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/mypagefav_hip_bt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:orientation="vertical"
            android:gravity="center"
            android:background="@drawable/rectangle"
            >
            <Button
                android:id="@+id/mypagefav_pick_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginTop="10dp"
                />
            <ImageView
                android:id="@+id/mypagefav_plus_iv"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/user"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                />
            <androidx.recyclerview.widget.RecyclerView
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:id="@+id/mypagefav_rv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:listitem="@layout/item_favdialog_workname"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>