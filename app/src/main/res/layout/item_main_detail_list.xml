<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginVertical="10dp"
    android:layout_marginHorizontal="5dp">
    <TextView
        android:id="@+id/item_main_detail_ilst_background_tv"
        android:layout_width="44dp"
        android:layout_height="26dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/mypagefav_part_backrounding">
    </TextView>
    <TextView
        android:id="@+id/item_main_detail_ilst__tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/item_main_detail_ilst_background_tv"
        app:layout_constraintLeft_toLeftOf="@id/item_main_detail_ilst_background_tv"
        app:layout_constraintRight_toRightOf="@id/item_main_detail_ilst_background_tv"
        app:layout_constraintBottom_toBottomOf="@id/item_main_detail_ilst_background_tv"
        android:background="@drawable/mypagefav_part_backrounding"
        android:gravity="center"
        android:textSize="9sp"
        android:textColor="@color/white"
        android:text="엉덩이">
    </TextView>
</androidx.constraintlayout.widget.ConstraintLayout>